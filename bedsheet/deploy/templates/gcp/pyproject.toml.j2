# {{ config.name }} - GCP Deployment
# Generated by: bedsheet generate --target gcp

[project]
name = "{{ config.name }}"
version = "0.1.0"
description = "{{ config.description or 'A Bedsheet agent deployment on GCP' }}"
requires-python = ">=3.11"
dependencies = [
    # Google ADK
    "google-adk>=0.1.0",
    # Google AI SDK
    "google-genai>=1.0.0",
    # Bedsheet
    "bedsheet-agents>=0.4.0rc1",
]

[build-system]
requires = ["hatchling"]
build-backend = "hatchling.build"
