# {{ config.name }} - GCP Deployment
# Generated by: bedsheet generate --target gcp
#
# This is a deployment project, not a distributable package.
# Use: uv run python -m google.adk.cli web .

[project]
name = "{{ config.name }}"
version = "0.1.0"
description = "{{ config.description or 'A Bedsheet agent deployment on GCP' }}"
requires-python = ">=3.11"
dependencies = [
    # Google ADK
    "google-adk>=0.1.0",
    # Google AI SDK (with pinned aiohttp for compatibility)
    "google-genai>=1.0.0",
    "aiohttp>=3.10.0",
]
