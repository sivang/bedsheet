# {{ config.name }} - Terraform Variables
# Generated by: bedsheet deploy --target gcp
#
# Copy this file to terraform.tfvars and fill in your values:
#   cp terraform.tfvars.example terraform.tfvars

# Required: Your GCP project ID
project_id = "{{ gcp.project or 'your-project-id' }}"

# GCP region for deployment
region = "{{ gcp.region or 'us-central1' }}"

# Set to true to allow public access (no authentication)
allow_unauthenticated = false

# Container image tag (usually "latest" or a git SHA)
image_tag = "latest"
